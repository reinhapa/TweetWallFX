            {
                "stepClassName": "org.tweetwallfx.devoxx18pl.steps.DevoxxShowSchedule"
            },
            {
                "stepClassName": "org.tweetwallfx.devoxx18pl.steps.DevoxxFlipOutTweets"
            },
            {
                "stepClassName": "org.tweetwallfx.devoxx18pl.steps.DevoxxFlipInTweets"
